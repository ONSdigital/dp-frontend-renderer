
<div class="page-intro background--astral">
    <div class="wrapper">
        <div class="col-wrap">
                <div class="col">
                    <h1 class="page-intro__title">Welcome to the Office for National Statistics</h1>
                    <p class="page-intro__content">The UK's largest independent producer of official statistics and the recognised national statistical institute of the UK.</p>
                </div>
        </div>
    </div>
</div>

<div class="background--gallery">
    <div class="wrapper">

        <h2>Latest published releases</h2>
        <ul class="list--neutral col-wrap margin-left--0">
            {{range .Data.Publications}}
              <li class="col col--lg-29 release margin-bottom--2 margin-right--1 background--white padding-top--1 padding-right--1 padding-bottom--1 padding-left--1">
                  <h3 class="release__title"><a href="{{ .URI}}">{{ .Title}}</a></h3>
                  <p class="release__content">{{ .ReleaseDate}}</p>
              </li>
            {{end}}
        </ul>

        <h2>Headline figures</h2>
        <ul class="list--neutral col-wrap margin-left--0">
            {{range .Data.HeadlineFigures}}
              <li class="col col--lg-14 headline-data margin-right--1 background--white padding-top--1 padding-right--1 padding-bottom--1 padding-left--1">
                  {{if .SparklineData}}
                    <div class="headline-data__sparkline">{{range .SparklineData}} {{ .Y}} {{end}}</div>
                  {{end}}
                  <h3 class="headline-data__title"><a href="{{ .URI}}">{{ .Title}}</a></h3>
                  <p class="headline-data__figure">
                      {{if .LatestFigure.PreUnit}}{{ .LatestFigure.PreUnit}} {{end}}
                      {{ .LatestFigure.Figure}}
                      {{if .LatestFigure.Unit}}{{ .LatestFigure.Unit}}{{end}}
                  </p>
                  <p class="flush">{{ .ReleaseDate}}</p>
              </li>
            {{end}}
            <li class="col col--lg-14 headline-data margin-top--2 margin-right--1 background--white padding-top--1 padding-right--1 padding-bottom--1 padding-left--1">
                <h3 class="headline-data__title"><a href="{{ .URI}}">Looking to download time series?</a></h3>
                <p class="flush">Try our time series explorer</p>
            </li>
        </ul>

        <h2>Featured content</h2>
        <ul class="list--neutral col-wrap margin-left--0">
            <li class="col col--lg-29 margin-bottom--2 margin-right--1 background--white padding-top--1 padding-right--1 padding-bottom--1 padding-left--1 height--24">
                <h3><a href="{{ .URI}}">UK Perspectives 2016</a></h3>
            </li>
            <li class="col col--lg-14 margin-bottom--2 margin-right--1 background--white padding-top--1 padding-right--1 padding-bottom--1 padding-left--1 height--24">
                <h3><a href="{{ .URI}}">Data science campus</a></h3>
            </li>
            <li class="col col--lg-14 margin-bottom--2 margin-right--1 background--white padding-top--1 padding-right--1 padding-bottom--1 padding-left--1 height--24">
                <h3><a href="{{ .URI}}">Independent review of UK economic statistics</a></h3>
            </li>
        </ul>

        <h2>Other official statistics</h2>
        <ul class="list--neutral col-wrap margin-left--0">
            {{range .Data.Other}}
              <li class="col col--lg-14 margin-bottom--2 margin-right--1 background--white padding-top--1 padding-right--1 padding-bottom--1 padding-left--1 height--24">
                  <h3><a href="{{ .URI}}">{{ .Title}}</a></h3>
              </li>
            {{end}}
        </ul>
    </div>
</div>
