<header>
    <a class="skiplink" href="#main" tabindex="0">{{ localise "SkipToContent" .Language 1 }}</a>
    <div id="pagePath" class="hide">{{.URI}}</div>

    {{if .ServiceMessage}}
        <div class="beta-banner">
            <div class="wrapper">
                <p>{{.ServiceMessage}}</p>
            </div>
        </div>
    {{end}}

    <div class="wrapper">
        <div class="header col-wrap">
            <div class="col col--lg-one-third col--md-one-third">
                <a id="logo-link" href="{{.TaxonomyDomain}}/">
                    {{`<!--[if lte IE 8]>` | safeHTML}}
                        <img class="logo" src="https://cdn.ons.gov.uk/assets/images/ons-logo/v2/ons-logo.png" alt="{{ localise "OfficeForNationalStatistics" .Language 1 }}">
                    {{`<![endif]-->` | safeHTML}}
                    {{`<!--[if gte IE 9]><!-->` | safeHTML}}
                        <img class="logo" src="https://cdn.ons.gov.uk/assets/images/ons-logo/v2/ons-logo.svg" alt="{{ localise "OfficeForNationalStatistics" .Language 1 }}">
                    {{`<![endif]-->` | safeHTML}}
                </a>
            </div>
            <div class="col col--lg-two-thirds col--md-two-thirds hide--sm print--hide language--js__container">
                <div class="language">
                    {{if eq .Language "cy"}}
                        <a class="language__link icon--hide" href="{{ domainSetLang .SiteDomain .URI "en" }}">{{ localise "EnglishToggle" .Language 1 }}</a> | {{ localise "WelshToggle" .Language 1 }}
                    {{ else }}
                        {{ localise "EnglishToggle" .Language 1 }} | <a class="language__link icon--hide" href="{{ domainSetLang .SiteDomain .URI "cy" }}">{{ localise "WelshToggle" .Language 1 }}</a>
                    {{end}}
                </div>
            </div>
            <div class="secondary-nav col col--lg-two-thirds col--md-two-thirds print--hide">
                <ul id="nav-secondary" class="secondary-nav__list js-nav-clone__list">
                    <li class="secondary-nav__item">
                        <a class="secondary-nav__link  js-nav-clone__link" href="{{.TaxonomyDomain}}/releasecalendar">{{ localise "ReleaseCalendar" .Language 1 }}</a>
                    </li>
                    <li class="secondary-nav__item">
                        <a class="secondary-nav__link  js-nav-clone__link" href="{{.TaxonomyDomain}}/methodology">{{ localise "Methodology" .Language 1 }}</a>
                    </li>
                    <li class="secondary-nav__item">
                        <a class="secondary-nav__link  js-nav-clone__link" href="{{.TaxonomyDomain}}/news">{{ localise "Media" .Language 1 }}</a>
                    </li>
                    <li class="secondary-nav__item">
                        <a class="secondary-nav__link  js-nav-clone__link" href="{{.TaxonomyDomain}}/aboutus">{{ localise "About" .Language 1 }}</a>
                    </li>
                    <li class="secondary-nav__item">
                        <a class="secondary-nav__link icon--hide js-nav-clone__link" target="_blank" rel="noopener noreferrer" href="https://blog.ons.gov.uk/">{{ localise "Blog" .Language 1 }}</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="primary-nav print--hide">
        <nav>
            <ul class="nav--controls">
                <li class="nav--controls__item">
                    <a href="#nav-primary" id="menu-toggle" aria-controls="nav-primary" class="nav--controls__menu {{if .SearchDisabled}} nav--controls__no-search{{end}}">
                        <span class="nav--controls__text">{{ localise "Menu" .Language 1 }}</span>
                    </a>
                </li>
                {{if not .SearchDisabled}}
                <li class="nav--controls__item ">
                    <a href="#nav-search" id="search-toggle" aria-controls="nav-search" class="nav--controls__search">
                        <span class="nav--controls__text">{{ localise "Search" .Language 1 }}</span>
                    </a>
                </li>
                {{end}}
            </ul>
