    {{if .BetaBannerEnabled}}
        <div class="banner">
            <div class="wrapper">
                <div class="col col--md-40 col--lg-44">
                    <span class="banner__text-icon">{{ localise "Beta" .Language 1 }}</span>
                    <div class="banner__body">
                        <p>{{ localise "BetaFeedback" .Language 1 | safeHTML }}</p>
                    </div>
                </div>
            </div>
        </div>
